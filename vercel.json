{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/(.*\\.html)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*\\.css)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*\\.js)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*\\.svg)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*\\.ico)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*\\.png)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}